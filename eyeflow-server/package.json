{
  "name": "eyeflow-server",
  "version": "1.0.0",
  "description": "EyeFlow - Universal Action Execution & Monitoring Platform (Nest.js)",
  "author": "EyeFlow Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "validate:manifests": "bash scripts/validate-connector-manifests.sh",
    "validate:schemas": "ajv compile -s schemas/llm-workflow-rules.schema.json",
    "check:semver": "ts-node scripts/check-semver.ts",
    "check:deprecations": "ts-node scripts/check-deprecations.ts",
    "verify:capability-semver": "ts-node scripts/verify-capability-semver.ts",
    "test:validation": "jest --testPathPattern=\"llm-validation\\.contract\\.spec\\.ts|catalog-manifest\\.spec\\.ts\" --coverage",
    "pre-submit-check": "npm run lint && npm run validate:manifests && npm run check:semver && npm test"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.10",
    "@nestjs/core": "^10.2.10",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/platform-socket.io": "^10.0.1",
    "@nestjs/swagger": "^11.2.6",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^10.0.1",
    "@types/ws": "^8.18.1",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "chokidar": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.3",
    "kafkajs": "^2.2.4",
    "mqtt": "^5.15.0",
    "nest-winston": "^1.10.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "protobufjs": "^8.0.0",
    "redis": "^5.11.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/config": "^4.0.3",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.4",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "ajv": "^8.12.0",
    "ajv-cli": "^5.0.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
