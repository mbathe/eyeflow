# Test Environment Configuration
NODE_ENV=test

# Database - Use SQLite in-memory for testing
DATABASE_HOST=:memory:
DATABASE_PORT=3306
DATABASE_USER=test
DATABASE_PASSWORD=test
DATABASE_NAME=:memory:
DATABASE_TYPE=better-sqlite3
DATABASE_SYNCHRONIZE=true

# Disable Kafka for testing
KAFKA_ENABLED=false
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=eyeflow-test

# JWT
JWT_SECRET=test-secret-key-change-in-production

# LLM Configuration (disabled for testing)
OPENAI_API_KEY=test-key
OPENAI_MODEL=gpt-4

# Agent Configuration
AGENT_API_URL=http://localhost:5000
AGENT_API_KEY=test-key

# Logging
LOG_LEVEL=error
